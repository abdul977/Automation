# WhatsApp Business API Configuration
# Copy this file to .env and update with your actual values

# Webhook verification token (set this in your Meta Developer Console)
VERIFY_TOKEN=your_verify_token_here

# Your Phone Number ID from WhatsApp Business API dashboard
# This is different from your Business Account ID
PHONE_NUMBER_ID=YOUR_PHONE_NUMBER_ID

# App Secret for webhook signature verification (optional but recommended)
APP_SECRET=your_app_secret_here
